{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts"
, "app/sponsors/whySponsor.jsx", "app/sponsors/sponsorshipTiers.jsx", "app/sponsors/pastSponsors.jsx", "app/sponsors/partnership.jsx", "app/sponsors/page.tsx", "app/sponsors/hero.jsx", "app/sponsors/flagshipEvent.jsx", "app/sponsors/final.jsx", "app/sponsors/credibility.jsx", "components/SplashScreen.jsx"  ],
  "exclude": ["node_modules"]
}
